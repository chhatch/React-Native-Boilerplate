    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chhatch.TestProject"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "android": {
      "package": "com.chhatch.TestProject"
    },
    "description": ""
  },
  "private": true
