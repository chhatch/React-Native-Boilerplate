{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "buildAndroid": "expo build:android -t apk",
    "buildIos": "expo build:ios -t ipa",
    "buildWeb": "expo build:web",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "./jest/jest.setupAfterEnv.js"
    ],
    "transformIgnorePatterns": [
