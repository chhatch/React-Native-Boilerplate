    "@react-native-picker/picker": "^1.9.10",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@types/uuid": "^8.3.0",
    "aws-amplify": "^3.3.6",
    "axios": "^0.21.0",
    "expo": "~39.0.2",
    "expo-asset": "~8.2.0",
    "expo-constants": "~9.2.0",
    "expo-font": "~8.3.0",
    "expo-linking": "^1.0.1",
